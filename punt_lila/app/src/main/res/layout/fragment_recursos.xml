<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.fragment_recursos">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/Purpura"
        android:backgroundTint="@color/Purpura"
        tools:context=".Conciertos">


        <TextView
            android:id="@+id/eventosApuntado2"
            style="@style/Titulo"
            android:layout_width="369dp"
            android:layout_height="332dp"
            android:background="@color/Morado"
            android:elegantTextHeight="false"
            android:text="@string/puntoLilaText"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.198" />

        <Button
            android:id="@+id/botonLila"
            android:layout_width="185dp"
            android:layout_height="56dp"
            android:backgroundTint="@color/Morado"
            android:text="@string/botonLila"
            android:textColor="@color/black"
            app:circularflow_defaultRadius="50dp"
            app:circularflow_radiusInDP="50dp"
            app:cornerRadius="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.736" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>